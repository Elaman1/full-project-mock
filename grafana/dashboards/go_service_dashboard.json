{
  "id": null,
  "title": "Go Service Dashboard",
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
      "title": "Request Duration (95th Percentile)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "95th percentile",
          "refId": "A"
        }
      ],
      "datasource": "Prometheus",
      "id": 1
    },
    {
      "type": "graph",
      "title": "HTTP Requests by Path",
      "targets": [
        {
          "expr": "sum by (path) (rate(http_requests_total[1m]))",
          "legendFormat": "{{path}}",
          "refId": "B"
        }
      ],
      "datasource": "Prometheus",
      "id": 2
    },
    {
      "type": "graph",
      "title": "Top 5 Slowest Paths (95th Percentile)",
      "targets": [
        {
          "expr": "topk(5, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, path)))",
          "legendFormat": "{{path}}",
          "refId": "C"
        }
      ],
      "datasource": "Prometheus",
      "id": 3
    },
    {
      "type": "stat",
      "title": "Login Success Count",
      "targets": [
        {
          "expr": "http_login_success_total",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "datasource": "Prometheus",
      "id": 4
    },
    {
      "type": "graph",
      "title": "Login Failures by Reason",
      "targets": [
        {
          "expr": "sum by (reason) (rate(http_login_failure_total[5m]))",
          "legendFormat": "{{reason}}",
          "refId": "B"
        }
      ],
      "datasource": "Prometheus",
      "id": 5
    }
  ]
}
